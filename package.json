    "gatsby-plugin-sharp": "^2.6.43",
    "gatsby-remark-images-contentful": "^2.3.19",
    "gatsby-remark-katex": "^3.3.14",
    "gatsby-remark-responsive-iframe": "^2.4.1",
    "gatsby-remark-vscode": "^3.1.0",
    "gatsby-source-contentful": "^3.0.3",
    "gatsby-transformer-remark": "^2.8.46",
    "gatsby-transformer-sharp": "^2.5.19",
    "katex": "^0.12.0",
    "lodash": "^4.17.20",
    "node-sass": "^7.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
